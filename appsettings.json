{
    "Serilog": {
        "Using": [ "Serilog.Sinks.Console" ],
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft": "Warning",
                "System": "Warning",
                "Microsoft.Hosting.Lifetime": "Warning"
            }
        },
        "WriteTo": [
            { "Name": "Console" }
        ]
    },
    "BwsGrpcApiSettings": {
        "Endpoint": "<GRPC-ENDPOINT - from portal.bioid.com>",
        "ClientId": "<CLIENTID - from portal.bioid.com>",
        "AccessKey": "<KEY - from portal.bioid.com>"
    },
    "AllowedHosts": "*"
}
